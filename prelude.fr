assume
  a == b  = ((#=) (a, b))
  a < b  = ((#<) (a, b))
  a > b  = ((#>) (a, b))
  a + b  = ((#+) (a, b))
  a - b  = ((#-) (a, b))
  a * b  = ((#*) (a, b))
  a / b  = ((#/) (a, b))
  a . b  = ((#.) (a, b))
  a ++ b  = ((#++) (a, b))
  a ; b  = ((#;) (a, b))
  a : b  = ((#:) (a, b))
  a !! b  = ((#!!) (a, b))
  head a b  = ((#head) a)
  tail a b  = ((#tail) a)
  nil? a b  = ((#nil?) a)
  fst  a b  = ((#fst) a)
  snd  a b  = ((#snd) a)
